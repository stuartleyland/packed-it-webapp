  "bugs": {
    "url": "https://github.com/stuartleyland/packed-it/issues"
  },
  "homepage": "https://github.com/stuartleyland/packed-it#readme",
  "dependencies": {
    "moment": "^2.13.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0",
    "express": "^4.13.3",
    "history": "^1.9.1"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.3.13",
    "npm-watch": "^0.1.4",
